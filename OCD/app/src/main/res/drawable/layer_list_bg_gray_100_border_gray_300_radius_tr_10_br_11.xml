        <shape android:shape="rectangle">
            <solid android:color="@color/gray_100"/>
            <corners android:bottomRightRadius="@dimen/_10pxh" android:topRightRadius="@dimen/_10pxh"/>
            <stroke android:color="@color/gray_300" android:width="@dimen/_1pxh"/>
